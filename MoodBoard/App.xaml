<Application x:Class="MoodBoardApp.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:MoodBoardApp"
             xmlns:utilitieslib="clr-namespace:UtilitiesLib;assembly=UtilitiesLib"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <utilitieslib:IsPositiveIntegerConverter x:Key="isIntConverter"></utilitieslib:IsPositiveIntegerConverter>
        <utilitieslib:IsPositiveDoubleConverter x:Key="isDoubleConverter"></utilitieslib:IsPositiveDoubleConverter>
        <utilitieslib:MyMultipleConverter x:Key="multiconverter"></utilitieslib:MyMultipleConverter>
    </Application.Resources>
</Application>
